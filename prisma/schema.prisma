generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id           String   @id @default(cuid())
  title        String
  date         DateTime
  frontcontent String
  maincontent  String
  belowtitle   String
  belowcontent String
  username     String
  postimage    String
  userimage    String
}
